# Staging Environment Configuration
# This file contains environment variables for staging environment
# Used for pre-production testing and QA

# Environment
NODE_ENV=production
EXPO_PUBLIC_ENV=staging

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://staging-api.LeafWise.com/api
EXPO_PUBLIC_PLANT_ID_API_URL=https://api.plant.id/v2
EXPO_PUBLIC_PLANT_ID_API_KEY=your_plant_id_api_key_staging

# Firebase Configuration - Staging
EXPO_PUBLIC_FIREBASE_API_KEY_STAGING=your_firebase_api_key_staging
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN_STAGING=LeafWise-staging.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID_STAGING=LeafWise-staging
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET_STAGING=LeafWise-staging.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID_STAGING=123456789
EXPO_PUBLIC_FIREBASE_APP_ID_STAGING=1:123456789:web:staging123456

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true
EXPO_PUBLIC_ENABLE_DEBUG_MODE=false
EXPO_PUBLIC_ENABLE_MOCK_DATA=false

# Logging Configuration
EXPO_PUBLIC_LOG_LEVEL=info
EXPO_PUBLIC_ENABLE_CONSOLE_LOGGING=true
EXPO_PUBLIC_ENABLE_REMOTE_LOGGING=true

# Performance Configuration
EXPO_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true
EXPO_PUBLIC_API_TIMEOUT_MS=8000
EXPO_PUBLIC_IMAGE_UPLOAD_TIMEOUT_MS=25000

# Development Tools (disabled in staging)
EXPO_PUBLIC_ENABLE_FLIPPER=false
EXPO_PUBLIC_ENABLE_REACTOTRON=false

# Staging Specific
EXPO_PUBLIC_STAGING_PASSWORD=staging_access_password